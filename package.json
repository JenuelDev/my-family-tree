{
  "name": "testning-only",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build:for:app": "run-p type-check build-only:for:app",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:for:app": "vite build --outDir ./fam-tree-app/src/dist --emptyOutDir --base ./",
