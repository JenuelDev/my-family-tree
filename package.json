    "firebase": "^9.23.0",
    "notiflix": "^3.2.6",
    "pinia": "^2.1.3",
    "primevue": "^3.30.0",
    "snap-storage": "^0.2.5",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@unocss/preset-web-fonts": "^0.53.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.63.6",
    "typescript": "~5.0.4",
    "unocss": "^0.54.0",
